@model UpdateContactDto
@{
    ViewData["Title"] = "Contact Güncelleme sayfası";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<form method="post">
    <input type="hidden" asp-for="ContactId" />
    <label>Map Url</label>
    <input type="text" asp-for="MapUrl" class="form-control" />
    <span asp-validation-for="MapUrl" class="text-danger"></span>
    <br />

    <label>Adres</label>
    <textarea asp-for="Address" class="form-control" rows="5"></textarea>
    <br />

    <label>Telefon</label>
    <input type="text" asp-for="Phone" class="form-control" />
    <br />

    <label>Email</label>
    <input type="text" asp-for="Email" class="form-control" />
    <br />


    <br />
    <button type="submit" class="btn btn-outline-primary">Kaydet</button>
</form>
